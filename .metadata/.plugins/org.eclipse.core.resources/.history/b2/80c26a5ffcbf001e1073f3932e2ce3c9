<mapper namespace="kr.hospital.mapper.AdminMapper">
	<!-- 관리자 로그인 -->
		<select id="adminLogin" resultType="kr.hospital.model.AdminVO">
			select admin_id, admin_pw
			from patient
			where patient_id = #{patient_id} and patient_pw = #{patient_pw}
		</select>
</mapper>